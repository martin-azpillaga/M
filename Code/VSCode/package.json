{
    "publisher": "azpillaga-world",
    "name": "m-ls",
    "displayName": "M language server",
    "description": "M language server",
    "version": "0.1.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/martin-azpillaga/M"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "m",
                "aliases": [
                    "M",
                    "m"
                ],
                "extensions": [
                    ".m"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "m",
                "path": "./snippets.json"
            }
        ],
        "grammars": [
            {
                "language": "m",
                "scopeName": "source.m",
                "path": "./m.tmLanguage.json"
            }
        ]
    },
	"activationEvents": [
		"onLanguage:m"
	],
	"main": "./extension.js"
}
