{
    "indentationRules":
    {
        "increaseIndentPattern": "{",
        "decreaseIndentPattern": "}"
    },
    "folding":
    {
        "markers": {"start": "{", "end": "}"}
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ]
}