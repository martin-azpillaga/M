{
  "publisher": "azpillaga-world",
  "name": "m-ls",
  "displayName": "M language server",
  "description": "M language server",
  "version": "0.4.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/martin-azpillaga/M"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "m",
        "aliases": [
          "M",
          "m"
        ],
        "extensions": [
          ".â²˜"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "m",
        "path": "./snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "m",
        "scopeName": "source.m",
        "path": "./m.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "m.open",
        "title": "Open Visual editor",
        "category": "M"
      },
      {
        "command": "m.restart",
        "title": "Restart M language server",
        "category": "M"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:m",
    "onCommand:m.open",
    "onCommand:m.restart"
  ],
  "main": "./debug.js",
  "dependencies": {
    "lookpath": "^1.1.0",
    "openurl": "^1.1.1",
    "vscode-languageclient": "6.1.3"
  }
}