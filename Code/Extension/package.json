{
	"name": "m-ls",
	"version": "0.7.2",
	"publisher": "minim-tools",
	"license": "AGPL-3.0-only",
	"displayName": "M language server",
	"description": "Data-driven language for game developers",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"data-driven",
		"programming language",
		"game development"
	],
	"preview": true,
	"qna": false,
	"icon": "logo.png",
	"homepage": "https://minim.tools/M",
	"repository": {
		"url": "https://github.com/martin-azpillaga/M"
	},
	"bugs": {
		"url": "https://github.com/martin-azpillaga/M/issues"
	},
	"contributes": {
		"languages": [
			{
				"id": "Ⲙ",
				"extensions": [
					".Ⲙ"
				],
				"filenamePatterns": [
					"**/*.Ⲙ"
				],
				"configuration": "./declarative features/brackets.json"
			}
		],
		"snippets": [
			{
				"language": "Ⲙ",
				"path": "./declarative features/snippets.json"
			}
		]
	},
	"activationEvents": [
		"onLanguage:Ⲙ",
		"workspaceContains:**/*.Ⲙ",
		"workspaceContains:Ⲙ.json"
	],
	"main": "./main.js",
	"dependencies": {
		"vscode-languageclient": "6.1.3"
	},
	"engines": {
		"vscode": "^1.43.0"
	}
}
